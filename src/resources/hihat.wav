    $(function() {
        var is_following;
        if ("False" == "True") is_following = true;
        else is_following = false;
        $("#follow_link_sound").click(function() {
            follow_unfollow_user(is_following);
        });
    });

    function follow_unfollow_user(is_following) {

        $("#follow_link_sound").unbind("click");

        var url;
        if (is_following) {
            url = '/follow/unfollow_user/Sascha%20Burghard/';
        }
        else {
            url = '/follow/follow_user/Sascha%20Burghard/';
        }

        $.ajax({

            type: 'POST',
            url: url,

        success: function () {

                is_following = !is_following;
                $("#follow_error").hide();

                if (is_following)
                    $("#follow_link_sound").text("Unfollow this user");
                else
                    $("#follow_link_sound").text("Follow this user");

                $("#follow_link_sound").click(function() {
                    follow_unfollow_user(is_following);
                });
            },

            error: function (data) {
                $("#follow_error").show();
                window.setTimeout(function(){
                    $("#follow_error").fadeOut(200);
                }, 3000);
            }

        });
    }

</script>


